{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-undef": "off",
        "no-console": "off",
        "no-alert": "off"
    }
}